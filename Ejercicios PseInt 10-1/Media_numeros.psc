Algoritmo Media_numeros
    Definir Promedio Como Real;
    Definir Suma,A,B,C Como Entero;
    Escribir "Ingrese tres numero para calcular el promedio:";
    Leer A,B,C;
    Escribir "Los valores ingresados son:";
    Escribir "-- ", A," -- ", B," -- ",C;
    Suma = A+B+C;
    Promedio = (Suma/3);
    Escribir "El promedio de los valores ingresados es: ", Promedio;
FinAlgoritmo